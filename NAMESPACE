#  All functions exported other than those starting with "."
exportPattern("^[^\\.]")


exportClasses("genomes")

S3method(plot,genomes)
S3method(summary,genomes)
S3method(update,genomes)
S3method(lines,genomes)
S3method(print,genomes)
